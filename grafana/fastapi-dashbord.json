{
    "dashboard": {
      "title": "FastAPI Dashboard",
      "panels": [
        {
          "type": "graph",
          "title": "API Request Duration",
          "targets": [
            {
              "expr": "http_request_duration_seconds",
              "legendFormat": "{{status}}",
              "refId": "A"
            }
          ]
        },
        {
          "type": "stat",
          "title": "API Request Count",
          "targets": [
            {
              "expr": "http_requests_total",
              "legendFormat": "{{method}} {{status}}",
              "refId": "B"
            }
          ]
        }
      ]
    }
  }
  